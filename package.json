{
	"name": "llm_audit-vscode",
	"description": "VS Code extension that uses large language models for code audit",
	"version": "0.0.2",
	"publisher": "Diratof Azdimarlow",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/TheSaintDiratof/llm_audit-vscode"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"type": "webview",
					"id": "llm_audit-vscode.responseView",
					"name": "llm_audit-vscode"
				}
			]
		},
		"commands": [
			{
				"command": "llm_audit-vscode.askLLM",
				"category": "llm_audit-vscode",
				"title": "Ask LLM"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/node": "^20.11.7",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.32.0",
		"pythonia": "^1.1.2",
		"typescript": "^5.3.2"
	},
	"dependencies": {
		"child_process": "^1.0.2"
	}
}
